{
    "manifest_version": 3,
    "name": "Duck Tracker",
    "version": "0.0.1",
    "description": "Motivates you to spend less time online with a threatening duck",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
